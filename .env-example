# HTTP Server
# Порт, на котором будет запущен HTTP сервер
HTTP_SERVER_PORT=8080

# Ключ API для защиты доступа к защищенным эндпоинтам
API_KEY=your-secret-api-key

# PostgreSQL
# Строка подключения к базе данных (DSN)
# Формат: postgres://user:password@host:port/dbname?sslmode=disable
DATABASE_URL=postgres://postgres:postgres@localhost:5432/geo_incidents?sslmode=disable

# Redis
# Адрес подключения к Redis серверу (host:port)
REDIS_ADDR=localhost:6379

# Worker
# URL адрес внешнего сервиса, куда будут отправляться вебхуки
WEBHOOK_URL=http://localhost:9090/webhook
